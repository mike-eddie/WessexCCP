<%= simple_form_for(@completed_outcome) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :teaching_session, label_method: :title_and_date %>
    <%= f.association :outcome, label_method: :outcome %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
